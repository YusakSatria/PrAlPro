#Input
pintu = int(input("Masukkan Panjang : "))
#Proses dan Output
for i in range(pintu):
    print("*",end=" ")
print(" ")

for i in range(pintu):
    print("*",end=" ")

    for j in range(1,pintu-1):
        if (j == (pintu-1)/2):
            print("*",end=" ")
        else:
            print(" ",end=" ")
    print("*")

for i in range(pintu):
    print("*",end=" ")
